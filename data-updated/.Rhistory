#Function for computing the kmers. Input -> FASTA File, Output -> Dataframe
kmer_df = function(filename,variant)
{
ffile = read.FASTA(filename)
kmer = kcount(ffile, k = 3)
target = rep((variant),length(ffile))
kmer_df1 = data.frame(kmer, target)
return(kmer_df1)
}
